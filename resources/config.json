			    "relationships": [
			        {
			            "relationshipType": "ManyToOne",
				        "objectType": "Country",
				        "objectName": "country",
				        "joins": [
				            {"columnName": "continent", "referencedColumnName": "continent"},
				            {"columnName": "country_name", "referencedColumnName": "countryname"}
				        ]
			        }
			    ]
			}
		]
	}
